{
  "compilerOptions": {
    "incremental": true,
    "rootDir": "./src",
    "target": "es2016",
    "lib": ["es2015", "dom"],
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./dist/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "useUnknownInCatchVariables": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false
  }
}
